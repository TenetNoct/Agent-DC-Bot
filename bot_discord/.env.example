# Configurações do Discord
DISCORD_TOKEN=seu_token_aqui

# Configurações do LM Studio
LM_STUDIO_API_URL=http://localhost:1234/v1

# Chaves de API para busca na web
GOOGLE_API_KEY=sua_chave_google_aqui
GOOGLE_CX=seu_cx_google_aqui
BING_API_KEY=sua_chave_bing_aqui

# Configurações de busca headless
# Defina como true para usar o Playwright em vez do Selenium (se disponível)
USE_PLAYWRIGHT=false
# Tempo de expiração do cache em horas
CACHE_EXPIRY=24
# Ative ou desative o cache (true/false)
CACHE_ENABLED=true

# Configurações de moderação automática
# Ative ou desative a moderação (true/false)
MODERATION_ENABLED=false
# Ative ou desative o filtro anti-spam (true/false)
MOD_ANTISPAM=false
# Ative ou desative o filtro anti-flood (true/false)
MOD_ANTIFLOOD=false
# Ative ou desative o filtro de palavras proibidas (true/false)
MOD_WORDFILTER=false
# Lista de palavras proibidas separadas por vírgula
BANNED_WORDS=

# Configurações de notificações
# Ative ou desative as notificações (true/false)
NOTIFICATIONS_ENABLED=false
# Método de notificação (telegram ou webhook)
NOTIFICATION_METHOD=webhook
# Token do bot do Telegram (se usar o método telegram)
TELEGRAM_TOKEN=
# ID do chat do Telegram (se usar o método telegram)
TELEGRAM_CHAT_ID=
# URL do webhook (se usar o método webhook)
WEBHOOK_URL=

# Configurações de log
LOG_LEVEL=INFO